{% extends 'base.html.twig' %}

{% block title %}Feedback{% endblock %}

{% block body %}
    <h1>Feedback</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ feedback.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ feedback.Name }}</td>
            </tr>
            <tr>
                <th>Level</th>
                <td>{{ feedback.level }}</td>
            </tr>
            <tr>
                <th>Comment</th>
                <td>{{ feedback.comment }}</td>
            </tr>
              <tr>
                <th>Rating</th>
                <td>{{ feedback.rating }}</td>
            </tr>
            
        </tbody>
    </table>

    <a href="{{ path('app_feed_back_index') }}">back to list</a>

    <a href="{{ path('app_feed_back_edit', {'id': feedback.id}) }}">edit</a>

    {{ include('feed_back/_delete_form.html.twig') }}
