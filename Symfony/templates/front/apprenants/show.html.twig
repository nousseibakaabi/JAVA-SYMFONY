

{% extends 'base-front.html.twig' %}


{% block title %}Level{% endblock %}

{% block body %}



    <section class="section-md bg-default decor-text" >
    <div class="container">
        <h4 class="heading-decorated">Apprenant</h4>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ apprenant.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ apprenant.name }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ apprenant.email }}</td>
            </tr>
           
            <tr>
                <th>StatutNiveau</th>
                <td>{{ apprenant.statutNiveau }}</td>
            </tr>
            <tr>
                <th>FormationEtudies</th>
                <td>{{ apprenant.formationEtudies }}</td>
            </tr>
            <tr>
                <th>Image</th>
                 <td>{{ apprenant.image}}</td>
                  <td><img src="{{ asset('uploads/images/' ~ apprenant.image) }}" height="300" width="300" alt="Img"></td>
                
            </tr>
        </tbody>
    </table>

    {{ include('apprenants/_delete_form.html.twig') }}
     </div>
</section>
{% endblock %}
